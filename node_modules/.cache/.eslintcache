\r\n        draggable\r\n        pauseOnHover\r\n        theme=\"light\"\r\n      />\r\n      <nav className=\"flex text-2xl pb-6 pt-8 gap-2 justify-center\">\r\n        <NavLink\r\n          to=\"/\"\r\n          exact\r\n          className=\"py-3 px-6 \"\r\n          activeClassName=\"bg-white shadow-sm text-blue-600\"\r\n        >\r\n          Filmler\r\n        </NavLink>\r\n        <NavLink\r\n          to=\"/listem\"\r\n          className=\"py-3 px-6 flex gap-x-5 items-center\"\r\n          activeClassName=\"bg-white shadow-sm text-blue-600\"\r\n        >\r\n          Listem\r\n          {Array.isArray(favMovies) && (\r\n            <p className=\"text-gray-50 text-sm bg-sky-600 py-1 px-2 rounded-full\">\r\n              {favMovies.length}\r\n            </p>\r\n          )}\r\n        </NavLink>\r\n      </nav>\r\n      <Switch>\r\n        <Route exact path=\"/\">\r\n          <Movie sira={sira} />\r\n\r\n          <div className=\"flex gap-3 justify-end py-3\">\r\n            {Array.isArray(movies) && movies.length === 0 && (\r\n              <div className=\"flex flex-col gap-3 justify-end py-3\">\r\n                <p className=\"text-center\">\r\n                  Görüntülenecek film bulunmamaktadır. Favoriler listesinden\r\n                  filmleri çıkararak bu sayfaya gelmelerini sağlayabilirsin.\r\n                </p>\r\n\r\n                <Link\r\n                  to=\"/listem\"\r\n                  className=\"py-3 px-6 bg-black shadow-sm text-white my-8 w-52 text-center mx-auto\"\r\n                >\r\n                  Favoriler\r\n                </Link>\r\n              </div>\r\n            )}\r\n            {sira > 0 && (\r\n              <button\r\n                className=\"select-none px-4 py-2 border border-blue-700 text-blue-700 hover:border-blue-500 hover:text-blue-500\"\r\n                onClick={handleFirstMovie}\r\n              >\r\n                Başa Dön\r\n              </button>\r\n            )}\r\n\r\n            {sira > 0 && (\r\n              <button\r\n                className=\"select-none px-4 py-2 border border-blue-700 text-blue-700 hover:border-blue-500 hover:text-blue-500\"\r\n                onClick={handlePrev}\r\n              >\r\n                Önceki\r\n              </button>\r\n            )}\r\n\r\n            {Array.isArray(movies) && sira < movies.length - 1 && (\r\n              <button\r\n                onClick={handleNext}\r\n                className=\"select-none px-4 py-2 border border-blue-700 text-blue-700 hover:border-blue-500 hover:text-blue-500\"\r\n              >\r\n                Sıradaki\r\n              </button>\r\n            )}\r\n\r\n            {Array.isArray(favMovies) &&\r\n              movies.length > 0 &&\r\n              !favMovies.includes(movies[sira]) && (\r\n                <button\r\n                  className=\"select-none px-4 py-2 bg-blue-700 hover:bg-blue-600 text-white\"\r\n                  onClick={handleAddFav}\r\n                >\r\n                  Listeme ekle\r\n                </button>\r\n              )}\r\n          </div>\r\n        </Route>\r\n\r\n        <Route path=\"/listem\">\r\n          <div>\r\n            {Array.isArray(favMovies) && favMovies.length === 0 && (\r\n              <div className=\"flex flex-col gap-3 justify-end py-3\">\r\n                <p className=\"text-center\">\r\n                  Görüntülenecek favori film bulunmamaktadır. Filmler\r\n                  listesinden favorilere ekleme yaparak bu sayfaya gelmelerini\r\n                  sağlayabilirsin.\r\n                </p>\r\n\r\n                <Link\r\n                  to=\"/\"\r\n                  className=\"py-3 px-6 bg-black shadow-sm text-white my-8 w-52 text-center mx-auto\"\r\n                >\r\n                  Fimler Listesi\r\n                </Link>\r\n              </div>\r\n            )}\r\n            {Array.isArray(favMovies) &&\r\n              favMovies.map((movie) => (\r\n                <FavMovie\r\n                  key={movie.id}\r\n                  title={movie.title}\r\n                  id={movie.id}\r\n                  movie={movie}\r\n                />\r\n              ))}\r\n          </div>\r\n          <button\r\n            onClick={handleRemoveFavs}\r\n            hidden={favMovies.length === 0}\r\n            className=\"underline\"\r\n          >\r\n            Favorileri Temizle\r\n          </button>\r\n        </Route>\r\n      </Switch>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\reportWebVitals.js",[],[],"C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\components\\FavMovie.js",["61"],[],"import { useDispatch, useSelector } from \"react-redux\";\r\nimport { removeFav } from \"../actions/favActions\";\r\nimport { addMovie } from \"../actions/movieActions\";\r\n\r\n\r\nexport default function FavMovie({ title, id }) {\r\n  const dispatch = useDispatch();\r\n  const favorites = useSelector((store) => store.favReducer.favorites);\r\n \r\n  const handleRemoveFavorites = () => {\r\n    dispatch(addMovie(id));\r\n    dispatch(removeFav(id));\r\n  };\r\n\r\n  return (\r\n    <div className=\"flex p-3 pl-4 bg-white mb-2 shadow items-center group\">\r\n      <div className=\"pr-4 flex-1\">{title}</div>\r\n      <button  onClick={handleRemoveFavorites} className=\"px-3 py-2 block text-sm rounded bg-rose-700 text-white opacity-30 group-hover:opacity-100\">\r\n        Çıkar\r\n      </button>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\components\\Movie.js",[],[],"C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\movies.js",[],[],"C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\actions\\favActions.js",[],[],"C:\\Users\\gozde\\OneDrive\\Belgeler\\GitHub\\fsweb-s10g3-redux-watchlist\\src\\actions\\m